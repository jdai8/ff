{
    "name": "ff",
    "manifest_version": 2,
    "version": "1.0",
    "description": "Tab finder",

    "browser_action": {
        "default_popup": "popup.html" 
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Space",
                "windows": "Ctrl+Space"
            }
        }
    },
    "permissions": ["tabs"]
}
